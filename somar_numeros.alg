algoritmo "somar_numeros"

var
     num1 , num2 , resp : real
inicio
// Seção de Comandos 
escreval("Calculadora para somar dois numeros")
escreval("insira o primeiro número")
leia(num1)
escreval("insira o segundo número")
leia(num2)
resp <- num1 + num2
escreva("O resultado da soma é" , resp)
fimalgoritmo
